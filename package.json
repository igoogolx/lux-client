    "qrcode": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-i18next": "^12.0.0",
    "react-inlinesvg": "^3.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.0.1",
