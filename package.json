{
  "name": "lux",
  "version": "0.3.9",
  "description": "A system network proxy tool",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack -c build/webpack.config.js --mode production --env NODE_ENV=production",
    "electron-builder": "electron-builder --publish never --config builder.json",
    "dev": "webpack -c build/webpack.config.js --env NODE_ENV=development --mode development",
    "start": "yarn dev && electron src/index.js",
    "make": "yarn build && yarn electron-builder",
    "make:dev": "yarn build:debugger && electron-builder --win --publish never --config builder.json --config.extraMetadata.name lux-dev",
    "format": "prettier ./app -write",
    "lint": "eslint --cache --ext .ts,.tsx app",
    "lint-fix": "eslint --ext .ts,.tsx --fix app",
    "dev:ui": "webpack serve -c build/webpack.ui.config.js --env NODE_ENV=development --mode development --hot --open",
    "build:ui": "webpack -c build/webpack.ui.config.js --env NODE_ENV=production --mode production"
  },
  "keywords": [],
  "author": "igoogolx",
  "license": "MIT",
